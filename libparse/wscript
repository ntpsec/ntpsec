def build(ctx):
    libparse_source = [
        "binio.c",
        "clk_computime.c",
        "clk_dcf7000.c",
        "clk_hopf6021.c",
        "clk_meinberg.c",
        "clk_rawdcf.c",
        "clk_rcc8000.c",
        "clk_schmid.c",
        "clk_sel240x.c",
        "clk_trimtaip.c",
        "clk_trimtsip.c",
        "clk_varitext.c",
        "clk_wharton.c",
        "data_mbg.c",
        "ieee754io.c",
        "info_trimble.c",
        "parse.c",
        "parse_conf.c",
        "trim_info.c",
    ]

    ctx(
        target="parse",
        features="c cstlib bld_include src_include libisc_include",
        source=libparse_source,
    )
